<script type="text/javascript">
var idleTime = 0;
var idleInterval = 0;
$(document).ready(function () {
//Increment the idle time counter every minute.
if ((sessionTimeout != null) && (sessionTimeout != 0)) {
idleInterval = setInterval("timerIncrement()", 60000); // 1 minute
}
//Zero the idle timer on mouse movement.
$(this).mousemove(function (e) {
idleTime = 0;
});
//Zero the idle timer on keypress movement.
$(this).keypress(function (e) {
idleTime = 0;
});
});
function timerIncrement() {
idleTime = idleTime + 1;
if (idleTime > (sessionTimeout - 1)) {
idleInterval = 0;
clearInterval(idleInterval);
document.location.href = "http://www.globalblue.com/profile/editProfile/?p=call_session_expiry&redirectUrl=http://www.globalblue.com/404/";
}
}
</script>
