<script type="text/javascript">
function getCookie(c_name)
{
var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++)
{
x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
x=x.replace(/^\s+|\s+$/g,"");
if (x==c_name)
{
return unescape(y);
}
}
}
function displayNotification()
{
$('#cookie-container').show();
}
function setJSCheckCookie(c_name,value,exdays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
var cookieValue = c_name + "=" + c_value+"; path=/";
document.cookie = cookieValue;
}
function checkCookie()
{
var cookieName="jsCookieCheck";
var cookieChk=getCookie(cookieName);
if (cookieChk!=undefined)
{
setJSCheckCookie(cookieName,cookieChk,365);	// reset the cookie to expire in a year.
$('#cookie-container').parent().remove();
}
else
{
// No cookie exists, so display the lightbox effect notification.
$('#cookie-container').parent().css('display', 'inline');
displayNotification();
}
}
$(document).ready(function(){
checkCookie();
$("button, input, li, a, img, p, span").click(function() {	setJSCheckCookie("jsCookieCheck","globalblue",365);
$('#cookie-container').slideUp();
});
});
</script>
